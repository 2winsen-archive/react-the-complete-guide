{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-console": [true, "warn"]
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "no-console": [true, "warn"]
  }
}